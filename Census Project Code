        "Q3 = df['age'].quantile(0.75)\n",
        "\n",
        "\n",
        "iqr = Q3-Q1\n",
        "\n",
        "lower_limit = Q1 - 1.5*iqr\n",
        "upper_limit = Q3 + 1.5*iqr\n",
        "\n",
        "Q1, Q3, iqr, lower_limit, upper_limit\n",
        "\n",
        "df2 = df[ (df['age'] < lower_limit) | ( df['age'] > upper_limit)].value_counts()\n",
